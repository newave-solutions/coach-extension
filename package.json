{
  "name": "coach-extension",
  "version": "1.0.0",
  "description": "Medical Interpreter Co-Pilot - Real-time AI assistance with 3-agent system",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build:icons": "node scripts/generateIcons.js"
  },
  "keywords": [
    "chrome-extension",
    "medical-interpreter",
    "ai-assistant",
    "transcription",
    "translation",
    "performance-evaluation"
  ],
  "author": "Newave Solutions",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
  }
}
